{
  "name": "misclientes-be",
  "version": "1.0.0",
  "description": "Proyect to join GM Transport",
  "main": "index.js",
  "scripts": {
    "test": "npm test",
    "dev": "cross-env NODE_ENV=development npx tsc&&cross-env NODE_ENV=development ts-node-dev --respawn --rs --ignore-watch .idea --files -- src/server.ts",
    "format": "prettier --write .",
    "generate-secret": "node -e 'console.log(require(\"crypto\").randomBytes(256).toString(\"base64\"))'",
    "reset-db": "cross-env NODE_ENV=development ts-node-dev src/scripts/reset-db.ts",
    "build": "npx tsc",
    "start-prod": "cross-env NODE_ENV=production&&set NODE_ENV=production&&npm install&&npx tsc&&node dist/server.js",
    "control-sync": "npx tsc && cross-env NODE_ENV=development ts-node-dev src/scripts/sync-sh/sync-sh.config.ts",
    "sync-telgoo-config": "npx tsc && cross-env NODE_ENV=development ts-node-dev src/scripts/telgoo/sync-telgoo.config.ts",
    "sync-tracking-config": "npx tsc && cross-env NODE_ENV=development ts-node-dev src/scripts/telgoo/sync-tracking/sync-tracking.config.ts",
    "sync-status-config": "npx tsc && cross-env NODE_ENV=development ts-node-dev src/scripts/telgoo/sync-status/sync-status.config.ts",
    "sync-transferout-config": "npx tsc && cross-env NODE_ENV=development ts-node-dev src/scripts/telgoo/sync-transferout/sync-transferout.config.ts",
    "generate-access-key": "node -e 'console.log(require(\"crypto\").randomBytes(10).toString(\"hex\"))'",
    "generate-api-key": "node -e 'const key=require(\"crypto\").randomUUID(); console.log(key); console.log(require(\"bcrypt\").hashSync(key, 8))'"
  },
  "author": "Luis Flores",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "luxon": "^3.4.4",
    "mysql2": "^3.9.7",
    "reflect-metadata": "^0.2.2",
    "ts-jest": "^29.1.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/luxon": "^3.4.2"
  }
}
